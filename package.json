{
	"name": "telecat",
	"type": "module",
	"version": "0.0.4",
	"private": true,
	"packageManager": "pnpm@9.15.4",
	"description": "Open Source Teleprompter App",
	"author": "MarcusUniverse",
	"license": "MIT",
	"engines": {
		"node": ">=23"
	},
	"scripts": {
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preinstall": "npx only-allow pnpm",
		"postinstall": "nuxt prepare",
		"lint": "eslint . --fix",
		"bump": "bumpp",
		"tauri": "tauri",
		"deploy": "gh-pages -d .output/public",
		"tauri:dev": "tauri dev",
