		"@vueuse/nuxt": "^12.5.0",
		"bumpp": "^10.0.2",
		"eslint": "9.17.0",
		"gh-pages": "^6.3.0",
		"nuxt-svgo": "^4.0.14",
		"sass": "^1.85.1",
		"typescript": "^5.7.3",
		"vite-plugin-vue-devtools": "^7.7.2"
