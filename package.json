		"tauri:build:debug": "tauri build --debug",
		"prebuild": "node scripts/prebuild.js",
		"postbuild": "node scripts/postbuild.js",
		"npm_patch": "npm version patch --no-git-tag-version",
		"npm_major": "npm version major --no-git-tag-version",
		"npm_minor": "npm version minor --no-git-tag-version"
	},
	"dependencies": {
		"@pinia/nuxt": "^0.10.1",
		"@tauri-apps/api": ">=2.2.0",
		"@tauri-apps/plugin-fs": "^2.2.0",
